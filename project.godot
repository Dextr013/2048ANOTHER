config_version=5

[application]

config/name="Cyberpunk 2048"
config/name_localized={
"ru": "Киберпанк 2048"
}
config/description="A futuristic take on the classic 2048 puzzle game with cyberpunk aesthetics and neon effects"
run/main_scene="res://scenes/Startup.tscn"
config/features=PackedStringArray("4.4")
config/icon="res://icon.png"

[autoload]

AppManager="*res://AppManager.gd"
LocalizationManager="*res://scripts/LocalizationManager.gd"
AssetManager="*res://scripts/AssetManager.gd"
SaveManager="*res://scripts/SaveManager.gd"
SoundManager="*res://scripts/SoundManager.gd"
ViewportHandler="*res://managers/ViewportHandler.gd"
LeaderboardManager="*res://scripts/LeaderboardManager.gd"
WebBus="*res://addons/WebBusPlugin/WebBus.gd"
GameReadyApI="*res://GameReadyAPI.gd"

[compression]

formats/zstd/long_distance_matching=true

[display]

window/size/viewport_width=1080
window/size/viewport_height=1920
window/stretch/mode="canvas_items"
window/handheld/orientation="portrait"
window/ios/allow_high_refresh_rate=false

[editor_plugins]

enabled=PackedStringArray("res://addons/WebBusPlugin/plugin.cfg")

